{
	"model_name":"Pago",
	"soft_deletes":"yes",
	"pk":"id",
	"localize":"yes",
	"table":"pagos",
    "fields": [
        {
            "name": "cantidadPago",
            "type": "number"
        },
        {
            "name": "fechaPago",
            "type": "date"
        },	
        {
            "name": "idCajero",
            "type": "bigint#unsigned"
        },
        {
            "name": "idCliente",
            "type": "bigint#unsigned"
        }

    ],
    "foreign_keys": [
        {
            "column": "idCajero",
            "references": "id",
            "on": "cajero",
            "onDelete": "cascade"
        },
        {
            "column": "idCliente",
            "references": "id",
            "on": "cliente",
            "onDelete": "cascade"
        }

    ],
    "relationships": [
        {
            "name": "cajero",
            "type": "belongsTo",
            "class": "App\\Cajero"           
        },
        {
            "name": "cliente",
            "type": "belongsTo",
            "class": "App\\Cliente"           
        }
    ]

	
}